using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace tba.DataStructures.MaxSubSum
{
    public class ResultsViewModel
    {
        public int[] Data { get; private set; }
        public int ResultN3 { get; private set; }
        public int ResultN2 { get; private set; }
        public int ResultN { get; private set; }

        public ResultsViewModel(int[] data, int resultN3, int resultN2, int resultN)
        {
            this.Data = data;
            this.ResultN3 = resultN3;
            this.ResultN2 = resultN2;
            this.ResultN = resultN;
        }
    }
}